{
  "name": "@gradienttim/elysia-versioning",
  "description": "A lightweight and flexible versioning plugin for ElysiaJS",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/GradientTim/elysia-versioning#readme",
  "bugs": {
    "url": "https://github.com/GradientTim/elysia-versioning/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GradientTim/elysia-versioning.git"
  },
  "scripts": {
    "build": "bunup --external elysia --format esm,cjs",
    "dev": "bunup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.1",
    "@types/bun": "^1.3.1",
    "bunup": "^0.15.12",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "elysia": "^1.4.13",
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
